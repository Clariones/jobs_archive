---------------------- functional methods for RS232 commands --------------------------
function calcCheckSum(strData, pStart, pEnd)
  local sum = 0;
  for i=pStart,pEnd do
    sum = sum + string.byte(strData,i,i)
  end
  return Utils.byte0(bit.bxor(sum, 0xFF) + 2)
end
function verifyChecksum(strData)
  local len = #strData - 2
  local calcCheckSum = calcCheckSum(strData, 1, len)
  local recvCheckSum, tailByte = string.byte(strData,len+1,len+2)
  return (recvCheckSum == calcCheckSum) and (tailByte == 0x03)
end
function makeData(tNames, tData)
  local cnt = 0
  local strData = ""
  if (tNames==nil) or (tData==nil) then return 0,nil end
  for id,data in pairs (tData) do
    for idx,name in pairs(tNames) do
      if (data[name] == nil) then return 0,nil end
      strData = strData .. string.char(Utils.byte0(tonumber(data[name])))
    end
    cnt = cnt + 1
  end
  return cnt,strData
end
function makeCommand(destAddr, srcAddr, CAT, ITEM, cmdCode, CNT, strData)
  local dataLen = #strData + 4
  local strCmd = string.char(0x02, destAddr, srcAddr, Utils.byte0(dataLen), Utils.byte1(dataLen), CAT, ITEM, cmdCode, CNT) .. strData
  local checkSum = calcCheckSum(strCmd, 1, #strCmd)
  return strCmd .. string.char(checkSum) .. string.char(3)
end
