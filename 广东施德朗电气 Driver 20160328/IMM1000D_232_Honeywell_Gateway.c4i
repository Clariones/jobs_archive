-- TODO ------------------------ Driver connection methods --------------------------
function isMatchedDriver(tParams, data, mustMatchParams, zeroMatchParams)
  for i,name in pairs(mustMatchParams) do
    if (tParams[name] ~= data[name]) then return false end
  end
  for i,name in pairs(zeroMatchParams) do
    if (tParams[name] ~= 0) and (tParams[name] ~= data[name]) then return false end
  end
  return true
end
function showConnectedDrivers()
  local itemTypes = {CONSTS.LIGHT, CONSTS.CURTAIN, CONSTS.SCENARIO}
  for i,itemType in pairs(itemTypes) do
    local curItemDrivers = DEVICE_CFG.connectedDrivers[itemType]
    if (curItemDrivers ~= nil) then
      print(itemType)
      local keyset = Utils.getSortedKeys(curItemDrivers)
      for id,drvId in pairs(keyset) do
        print("\t" .. Utils.tableToString(curItemDrivers[keyset[id]]))
      end
    else
      print(itemType .. "\n\tNo any connected drivers")
    end
  end
end
function findConnectedDriver(strType, tParams)
  local mustMatchParams = {CONSTS.PARAM.HG, CONSTS.PARAM.LC}
  local zeroMatchParams = {CONSTS.PARAM.ZONE}
  if (strType ~= CONSTS.SCENARIO) then table.insert(zeroMatchParams,CONSTS.PARAM.GROUP) end
  local connectedDrivers = DEVICE_CFG.connectedDrivers[strType]
  if (connectedDrivers == nil) then return {} end
  local result = {}
  for drvId,data in pairs(connectedDrivers) do
    if (isMatchedDriver(tParams, data, mustMatchParams, zeroMatchParams)) then
      result[drvId] = data
    end
  end
  return result
end
function _refreshDriverConnections()
  DEVICE_CFG.connectedDrivers = {}
  
  local devs = C4:GetBoundConsumerDevices(0, DEVICE_CFG.CONNECTION_IMM1000D)
  local devId = C4:GetDeviceID()
  for id,name in pairs(devs) do
    C4:SendToDevice(id,"GetDriverInfo",{deviceId=devId})
  end
end
function _connectDriver(tParams)
  -- tParams = {gatewayAddr=1, controllerAddr=0xE1, itemType="LIGHT", zoneNumber=1, groupNumber=2, driverId=334}
  local driverId = tonumber(tParams[CONSTS.PARAM.DRIVER])
  if (DEVICE_CFG.connectedDrivers[tParams[CONSTS.PARAM.ITEM]] == nil) then DEVICE_CFG.connectedDrivers[tParams[CONSTS.PARAM.ITEM]] = {} end
  local itemConnected = DEVICE_CFG.connectedDrivers[tParams[CONSTS.PARAM.ITEM]]
  local driver = itemConnected[driverId]
  if (driver == nil) then
    itemConnected[driverId] = {}
    driver = itemConnected[driverId]
  end
  local connectCaredParamNames = {CONSTS.PARAM.HG,CONSTS.PARAM.LC,CONSTS.PARAM.ITEM,CONSTS.PARAM.ZONE,CONSTS.PARAM.GROUP,CONSTS.PARAM.DRIVER}
  for i,name in pairs(connectCaredParamNames) do
    driver[name] = tonumber(tParams[name]) or tParams[name]
  end
end
function LUA_ACTION.showConnectedDrivers()
  showConnectedDrivers()
end
function LUA_ACTION.refreshConnections()
  _refreshDriverConnections()
end
function EX_CMD.connectDriver(tParams)
  _connectDriver(tParams)
end

