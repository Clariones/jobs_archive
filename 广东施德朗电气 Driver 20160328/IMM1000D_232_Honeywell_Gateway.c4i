function handlePackageError(idBinding, msg)
  local strMsg = "Package Error:"
  local errorCodeMap = {[0xF0]="STX", [0xF1]="DES",[0xF2]="SOU",[0xF3]="SIZE", [0xF4]="CAT", [0xF5]="DEV",[0xF6]="CMD", [0x07]="CNT", [0x08]="CRC",[0x09]="ETX"}
  local errorCode = errorCodeMap[string.byte(msg.DATA)] or "Unknown"
  strMsg = strMsg .. "\n\terror code=".. errorCode
  if (#(msg.DATA) ~= 12) then
    strMsg = strMsg .. "\n\tinvalid body, unknown package"
  else
    local stx,des,sou,sizeLo,sizeHigh,cat,dev,cmd,cnt,crc,etx = string.byte(msg.DATA,2,12)
    strMsg = strMsg .. string.format("\n\tSTX=%02X DES=%02X SOU=%02X SIZE=%d, CAT=%02X DEV=%02X CMD=%02X CNT=%02X CRC=%02X ETX=%02X",
      stx,des,sou,(sizeLo+sizeHigh*256),cat,dev,cmd,cnt,crc,etx)
  end
  if Dbg:IsLoggable(2) then
    Dbg:Warn(strMsg)
  else
    print(strMsg)
  end
end
