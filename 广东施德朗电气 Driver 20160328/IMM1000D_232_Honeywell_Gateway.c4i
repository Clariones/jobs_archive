function handleLightQueryResponse(idBinding, msg)
  local dataLen = msg.CNT * 5
  if (#(msg.DATA) ~= dataLen) then Dbg:Error("Invalid message body length") return end
  for i=1,msg.CNT do
    local tParams = {[CONSTS.PARAM.CMDCODE]="QueryResponse", [CONSTS.PARAM.HG]=msg.DES,[CONSTS.PARAM.LC]=msg.SOU}
    local curDataStartPos = (i-1)*5
    tParams[CONSTS.PARAM.ZONE] = string.byte(msg.DATA,curDataStartPos+1, curDataStartPos+1)
    tParams[CONSTS.PARAM.GROUP] = string.byte(msg.DATA,curDataStartPos+2, curDataStartPos+2)
    tParams[CONSTS.PARAM.RESPCODE] = COMMAND.RESPONSE.CONDITION[string.byte(msg.DATA,curDataStartPos+3, curDataStartPos+3)] or "Unknown"
    tParams[CONSTS.PARAM.POWER] = COMMAND.RESPONSE.POWER[string.byte(msg.DATA,curDataStartPos+4, curDataStartPos+4)] or "Unknown"
    tParams[CONSTS.PARAM.BRIGHT] = string.byte(msg.DATA,curDataStartPos+5, curDataStartPos+5)
    updateLightState(tParams)
  end
end
