end
------------------- RS232 response related -----------------------
function decodeMessage(strData)
  local len = #strData
  if (len < 12) then return nil end
  if (string.byte(strData,1,1) ~= 0x02) then Dbg:Trace("Not start with 0x02") return nil end 
  if (not verifyChecksum(strData)) then Dbg:Trace("CRC error") return nil end
  local bodySize = Utils.LEtoUShort(string.sub(strData,4,5))
  if (bodySize ~= (len-7)) then Dbg:Trace("Length not match SIZE") return nil end
  local msg = {}
  msg.DES = string.byte(strData, 2,2)
  msg.SOU = string.byte(strData, 3,3)
  msg.CAT = string.byte(strData,  6,6)
  msg.ITEM = string.byte(strData,  7,7)
  msg.CMD = string.byte(strData,  8,8)
  msg.CNT = string.byte(strData,  9,9)
  msg.DATA = string.sub(strData,10,len-2)
  return msg
end
function OnRS232MessageRecieved(idBinding, strData)
  local msg = decodeMessage(strData)
  if (msg == nil) then 
    if (Utils.dumpData == Utils.DUMP_DATA_ALL) then
      print("There is some data in RS232 bus, but not for me:")
      hexdump(strData)
    end
    return
  end
  if (Utils.dumpData == Utils.DUMP_DATA_RECIEVED) or (Utils.dumpData == Utils.DUMP_DATA_ALL) then
    print("Recieved data from RS232 bus:")
    hexdump(strData)
    print(Utils.tableToString(msg))
  end
  cmdResponseProcess(idBinding, msg)
end
