function makeAndSentDebugCommand(dbgCode, mills, strCmd)
  local secParam = math.floor(tonumber(mills/100))
  local strInCmd = tohex(strCmd)
  local checksum = calcCheckSum(strInCmd,1,#strInCmd)
  local strCmd = strInCmd .. string.char(checksum) .. string.char(3)
  local strDebugCmd = string.char(0, dbgCode, #strCmd+1, secParam) .. strCmd .. string.char(0xFF, 0xDD)
  hexdump(strDebugCmd)
  C4:SendToSerial(1,strDebugCmd)
end
function SetAutoReport(mills, strCmd)
  makeAndSentDebugCommand(0xE1, mills, strCmd)
end
function SetResponse(mills, strCmd)
