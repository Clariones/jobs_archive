  copyParams(tParams, tCmdParam, {CONSTS.PARAM.HG, CONSTS.PARAM.LC}, paramsRules)
  copyParams(tParams, tCmdParam.data[1], {CONSTS.PARAM.ZONE, CONSTS.PARAM.GROUP}, paramsRules)
  refreshConnection(tParams, CONSTS.LIGHT)
  sendOneCommand("cmdQueryLight", tCmdParam)
