function refreshConnection(tParams, driverType)
  tParams[CONSTS.PARAM.ITEM] = driverType
  _connectDriver(tParams)
  return DEVICE_CFG.connectedDrivers[driverType][tonumber(tParams[CONSTS.PARAM.DRIVER])]
end
function copyParams(tFrom, tTo, tParams, tRule)
  for i,name in pairs(tParams) do
    local rule = tRule[name]
    if (rule.type == "Number") then
      tTo[name] = tonumber(tFrom[name])
    elseif (rule.type == "List") then
      tTo[name] = tRule[name].values[tFrom[name]]
    end
    if (tTo[name] == nil) then
      print("Cannot copy param " .. name .." with rule " .. Utils.tableToString(tRule[name]))
    end
  end
end
