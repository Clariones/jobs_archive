function cmdResponseProcess(idBinding, msg)
  local deviceHandler = responseHandlers[msg.ITEM]
  if (deviceHandler == nil) then 
    Dbg:Error("Unknow device message to handle: " .. msg.ITEM)
    return
  end
  local cmdHandler = deviceHandler[msg.CMD]
  if (cmdHandler == nil) then
    Dbg:Error("Unknow command message to handle: " .. msg.CMD)
    return
  end
  cmdHandler(idBinding, msg)
end
