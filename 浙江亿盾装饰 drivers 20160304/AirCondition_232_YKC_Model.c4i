        end,
      responseHandler = commonResponseHandler,
   },
   cmdSetTemperature = {
      -- TODO: handle lock-key-press: 0xA6
      cmdCode = 0xA6,
      command = function(cmdCode, tParams)
          local d0,d1,d2,d3 = 0,0,0,0
          d2 = DriverLib.byte0(tParams["SettingTemperature"])
          return makeCmd(cmdCode, d0,d1,d2,d3)
        end,
      responseHandler = commonResponseHandler,
   },
   cmdSetCompensationTmpr = {
      -- TODO: handle lock-key-press: 0xA6
