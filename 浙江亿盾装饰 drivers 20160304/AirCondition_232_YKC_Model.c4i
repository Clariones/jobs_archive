function toSignedByte(curByte)
  if (curByte > 0x7F) then
    return curByte - 0x100
  else
    return curByte
  end
end
function calcYkcChecksum(strData, fromPos, toPos)
  --TODO calculate checksum
  local rst = 0;
  for i=fromPos,toPos do
    rst = rst + string.byte(strData,i,i)
  end
  rst = bit.band(rst, 0xFF)
  rst = bit.bxor(rst, 0xA5)
  return rst
end
function verifyYkcChecksum(strData)
  local len = #strData
  local checksum = calcYkcChecksum(strData,1,len-1)
  return checksum == string.byte(strData,len,len)
end

function makeCmd(cmdCode, d0, d1, d2, d3)
  local tgtDvcAddress = deviceAddress
  if (cmdCode ~= 0xA0) and (PropertyHandlingSpecialist:getPropertyValue("BroadcastMode")) then
    tgtDvcAddress = 0x0000
  end
  local strCmd = string.format("%02X %02X %02X %02X %02X %02X %02X",
      cmdCode, DriverLib.byte1(tgtDvcAddress), DriverLib.byte0(tgtDvcAddress),
      d0, d1, d2, d3)
  local hexCmd = tohex(strCmd)
  local checksum = calcYkcChecksum(hexCmd, 1, 7)
  return hexCmd .. string.char(checksum)
end

