function covertExCmdParams(tParams)
  if (tParams == nil) then return nil end
  local result = {}
  for name,value in pairs(tParams) do
    local info = Mine_driverVars[name]
    if (info == nil) then print("Unknow params " .. name) return nil end
    local cmdValue = nil
    if (info.dataType == "list") then
      cmdValue = info.valueList[value]
    else
      cmdValue = tonumber(value)
      if (info.range ~= nil) then
        if (cmdValue < info.range[1]) or (cmdValue > info.range[2]) then
          cmdValue = nil
        end
      end
    end
    if (cmdValue == nil) then
      print("Invalid value [" .. (tostring(value) or "nil") .. "] for params " .. name)
      return nil
    end
    result[name] = cmdValue
  end
  return result
end
