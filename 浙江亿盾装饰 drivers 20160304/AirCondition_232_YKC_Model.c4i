function EX_CMD.DRIVER_EXTENDS_CMDS(strCommand, tParams)
  Dbg:Debug("ExecuteCommand function called with : " .. strCommand .."(" .. DriverLib.tableToString(tParams,"") .. ")") 
  if (tParams == nil) then
    dbg ("From ExecuteCommand Function - Unutilized command: " .. strCommand)
  end

  if (strCommand == "HVACMODE_CHANGED") then
    local HVACMode = tParams["HVACMODE"]
    if (HVACMode ~= HvacProxyData.mode) then
      HvacProxyData.mode = HVACMode
      SendToProxy(PROXY_ID, strCommand, tParams)
    end
  elseif (strCommand == "FAN_STATE") then
    local fanState = tParams["STATE"]
    if (fanState ~= HvacProxyData.fanState) then
      HvacProxyData.fanState = fanState
      SendToProxy(PROXY_ID, strCommand, tParams)
    end
  elseif (strCommand == "SCALE_CHANGED") then
    local scale = tParams["SCALE"]
    if (scale ~= HvacProxyData.scale) then
      HvacProxyData.scale = scale
      SendToProxy(PROXY_ID, strCommand, tParams)
    end
  elseif (strCommand == "FANMODE_CHANGED") then
    local fanMode = tParams["FANMODE"]
    if (fanMode ~= HvacProxyData.fanMode) then
      HvacProxyData.fanMode = fanMode
      SendToProxy(PROXY_ID, strCommand, tParams)
    end
  elseif (strCommand == "HEAT_SETPOINT_CHANGED") then
    local setPoint = tonumber(tParams["SETPOINT"])
    if (setPoint ~= HvacProxyData.heatPoint) then
      HvacProxyData.heatPoint = setPoint
      SendToProxy(PROXY_ID, strCommand, tParams)
    end
  elseif (strCommand == "COOL_SETPOINT_CHANGED") then
    local setPoint = tonumber(tParams["SETPOINT"])
    if (setPoint ~= HvacProxyData.coolPoint) then
      HvacProxyData.coolPoint = setPoint
      SendToProxy(PROXY_ID, strCommand, tParams)
    end
  elseif (strCommand == "TEMPERATURE_CHANGED") then
    local temperature = tonumber(tParams["TEMPERATURE"])
    if (temperature ~= HvacProxyData.temperature) then
      HvacProxyData.temperature = temperature
      SendToProxy(PROXY_ID, strCommand, tParams)
    end
  else
    print("Un-handled command " .. strCommand)
    return false
  end
  return true
end

function PRX_CMD.DRVIER_EXTENDS_CMD(idBinding, strCommand, tParams)
--function ReceivedFromProxy(idBinding, strCommand, tParams)
