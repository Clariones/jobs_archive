PROXY_ID = 5001
function CommandInterpreter(idBinding, strCommand, tParams)
  if (strCommand ~= nill) then
    if(tParams == nill) then    -- initial table variable if nill
      tParams = {}
    end
    -- notifies
    if (strCommand == "CURRENT_SETTINGS") then
      local tParams = {
        HVACMODE = HvacProxyData.mode,
        FANMODE = HvacProxyData.fanMode,
        HOLDMODE = "OFF",
        HEAT_SETPOINT = HvacProxyData.heatPoint,
        COOL_SETPOINT = HvacProxyData.coolPoint,
        SCALE = HvacProxyData.scale,
        CURRTEMP = HvacProxyData.temperature,
      }
      SendToProxy(PROXY_ID, strCommand, tParams)
    elseif (strCommand == "SCALE_CHANGED") then
      HvacProxyData.scale = tParams["SCALE"]
      SendToProxy(PROXY_ID, strCommand, tParams)
    elseif (strCommand == "SET_MODE") then
      local HVACMode = tParams["HVACMODE"]
      if (HVACMode ~= HvacProxyData.mode) then
        changeMode(HVACMode)
        proxySetMode()
      end
    elseif (strCommand == "SET_FAN_MODE") then
      local fanMode = tParams["FANMODE"]
      if (fanMode ~= HvacProxyData.fanMode) then
        HvacProxyData.fanMode = fanMode
        SendToProxy(PROXY_ID, "FANMODE_CHANGED", tParams)
        proxySetFanMode()
      end
    elseif (strCommand == "SET_HEAT_SETPOINT")  then
      local heatSetPoint = tonumber(tParams["SETPOINT"])
      if (heatSetPoint ~= HvacProxyData.heatPoint) then
        HvacProxyData.heatPoint = heatSetPoint
        SendToProxy(PROXY_ID, "HEAT_SETPOINT_CHANGED", tParams)
        if (HvacProxyData.mode == "HEAT") then
          proxySetTemperature()
        end
      end
    elseif (strCommand == "SET_COOL_SETPOINT") then
      local coolSetPoint = tonumber(tParams["SETPOINT"])
      if (coolSetPoint ~= HvacProxyData.coolPoint) then
        HvacProxyData.coolPoint = coolSetPoint
        SendToProxy(PROXY_ID, "COOL_SETPOINT_CHANGED", tParams)
        proxySetTemperature()
      end
    elseif (strCommand == "SET_SCALE") then
      local scale = tParams["SCALE"]
      changeScale(scale)
--    else
--      SendToProxy(PROXY_ID, strCommand, tParams)
    end
  end
end

function updateSetPoints()
  local tParams={}
  tParams.SETPOINT = toInt(HvacProxyData.coolPoint)
  SendToProxy(PROXY_ID, "COOL_SETPOINT_CHANGED", tParams)
  tParams.SETPOINT = toInt(HvacProxyData.heatPoint)
  SendToProxy(PROXY_ID, "HEAT_SETPOINT_CHANGED", tParams)
end
function changeMode(HVACMode)
  HvacProxyData.mode = HVACMode
  local tParams = {HVACMODE=HVACMode}
  SendToProxy(PROXY_ID, "HVACMODE_CHANGED", tParams)
  --updateSetPoints()
end
function changeScale(newScale)
  if (newScale == HvacProxyData.scale) then return end
  HvacProxyData.scale = newScale
  if (HvacProxyData.scale == "C") then
    HvacProxyData.temperature =TemperatureF2C(HvacProxyData.temperature)
    HvacProxyData.coolPoint = TemperatureF2C(HvacProxyData.coolPoint)
    HvacProxyData.heatPoint = TemperatureF2C(HvacProxyData.heatPoint)
  else
    HvacProxyData.temperature = TemperatureC2F(HvacProxyData.temperature)
    HvacProxyData.coolPoint = TemperatureC2F(HvacProxyData.coolPoint)
    HvacProxyData.heatPoint = TemperatureC2F(HvacProxyData.heatPoint)
  end
  local tParams1={SCALE = HvacProxyData.scale}
  SendToProxy(PROXY_ID, "SCALE", tParams1)
  local tParams2={TEMPERATURE = toInt(HvacProxyData.temperature+0.5)}
  SendToProxy(PROXY_ID, "TEMPERATURE_CHANGED", tParams2)
  
  updateSetPoints()
end

function proxySetMode()
  print("proxySetMode()")
  print(HvacProxyData.mode)
  local mode = HvacProxyData.mode
  local modeCode = 0xE1
  if (mode == "AUTO") then
    modeCode = 0
  elseif (mode == "OFF") then
    cmdStartStop({RunningState=0})
    PropertyHandlingSpecialist:updateProperty("RunningState",0)
    return
  elseif (mode == "HEAT") then
    modeCode = 1
  elseif (mode == "COOL") then
    modeCode = 0
  else
    print("Unsupport mode " + mode)
    return
  end
  local curRunning = PropertyHandlingSpecialist:getPropertyValue("RunningState")
  if (curRunning ~= 1) then
    PropertyHandlingSpecialist:updateProperty("RunningState",1)
    cmdStartStop({RunningState=1})
    DriverLib.sleep(0.5)
  end
  PropertyHandlingSpecialist:updateProperty("WorkingMode", modeCode)
  cmdSetWorkMode({WorkingMode=modeCode})
end

function proxySetTemperature()
  local tgtTempr = 0
  if (HvacProxyData.mode == "HEAT") then
    tgtTempr = HvacProxyData.heatPoint
  else
    tgtTempr = HvacProxyData.coolPoint
  end
  if (HvacProxyData.scale == "F") then
    tgtTempr = toInt(TemperatureF2C(tgtTempr))
  end
  if (tgtTempr < 5) then tgtTempr = 5 end
  if (tgtTempr > 35) then tgtTempr = 35 end
  PropertyHandlingSpecialist:updateProperty("SettingTemperature", tgtTempr)
  cmdSetTemperature({SettingTemperature = tgtTempr})
end

function proxySetFanMode()
  local fanMode = HvacProxyData.fanMode
  local value = 0
  if (fanMode == "LOW") then
    value = 3
  elseif (fanMode == "MED") then
    value = 2
  elseif (fanMode == "HIGH") then
    value = 1
  elseif (fanMode == "AUTO") then
    value = 0 -------TODO
  else
    print("Unsupported fan mode " .. tostring(fanMode or "nil"))
    return
  end
  PropertyHandlingSpecialist:updateProperty("WindSpeed", value)
  cmdSetWindSpeed({WindSpeed=value})
end

function updateProxyData(result)
  -- TODO: updateProxyData
  local curValue = nil
  -- mode
  if (result.RunningState == 0) then
    curValue = "OFF"
  else
    if (result.WorkingMode == 1) then
      curValue = "HEAT"
    elseif (result.WorkingMode == 0) then
      curValue = "COOL"
    end
  end
  if (curValue ~= nil) and (curValue ~= HvacProxyData.mode) then
    changeMode(curValue)
  end
  
  -- setpoint
  if (result.SettingTemperature ~= nil) then
    curValue = tonumber(result.SettingTemperature)
    if (HvacProxyData.scale == "F") then
      curValue = TemperatureC2F(curValue)
    end
    if (HvacProxyData.mode == "HEAT") then
      HvacProxyData.heatPoint = curValue
    else
      HvacProxyData.coolPoint = curValue
    end
    updateSetPoints()
  end
  
  -- temperature
  if (result.RoomTemperature ~= nil) then
    curValue = result.RoomTemperature
    if (HvacProxyData.scale == "F") then
      curValue = TemperatureC2F(curValue)
    end
    HvacProxyData.temperature = curValue
    SendToProxy(PROXY_ID, "TEMPERATURE_CHANGED", {TEMPERATURE = HvacProxyData.temperature})
  end
  
  --fan mode
  if (result.WindSpeed ~= nil) then
    curValue = nil
    if (result.WindSpeed == 3) then
      curValue = "LOW"
    elseif (result.WindSpeed == 2) then
      curValue = "MED"
    elseif (result.WindSpeed == 1) then
      curValue = "HIGH"
    elseif (result.WindSpeed == 0) then
      curValue = "AUTO"
    end
    if (curValue ~= nil) and (curValue ~= HvacProxyData.fanMode) then
      HvacProxyData.fanMode = curValue
      SendToProxy(PROXY_ID, "FANMODE_CHANGED", {FANMODE = HvacProxyData.fanMode})
    end
  end
end



